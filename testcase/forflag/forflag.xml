<?xml version="1.0" encoding="UTF-8"?>
<project name="forflag" default="forflag2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="forflag2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${dita.dir}/testdata/forflag/outcase/index.ditamap"/>
      <property name="dita.input.valfile"
                location="${dita.dir}/testdata/forflag/filter/flag.ditaval"/>
      <property name="output.dir" location="${dita.dir}/testresult/forflag/outcase/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/forflag/outcase/xhtml"/>
    </ant>
  </target>
</project>