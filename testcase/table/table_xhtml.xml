<?xml version="1.0" encoding="UTF-8"?>
<project name="table_xhtml" default="table2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="table2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/table.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/table/xhtml"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/table/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>