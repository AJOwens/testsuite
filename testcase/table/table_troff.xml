<?xml version="1.0" encoding="UTF-8"?>
<project name="table_troff" default="table2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="table2troff" if="run.troff" unless="skip.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/table.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/table/troff"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/table/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>