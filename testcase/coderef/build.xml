<?xml version="1.0" encoding="UTF-8" ?>
<project name="sample_all" default="all" basedir=".">
  <property name="currentdir" location="${basedir}"/>
  <target name="all">
    <echo message="${currentdir}" level="info"/>
    <sequential>
      <subant>
        <fileset dir="${currentdir}" includes="coderef_*.xml"/>
      </subant>
    </sequential>
  </target>
</project>
