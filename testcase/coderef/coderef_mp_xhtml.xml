<?xml version="1.0" encoding="UTF-8"?>
<project name="coderef_mp_xhtml" default="coderef_mpxhtml" basedir=".">
   <dirname property="test.basedir" file="${ant.file.coderef_mp_xhtml}"/>
   <property name="test.name" value="coderef"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="coderef_mpxhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml" target="init">
         <property name="args.input" location="${test.basedir}/src/mp.ditamap"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="args.logdir" location="${result.dir}/mp/log"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="xhtml"/>
         <property name="clean.temp" value="no"/>
      </ant>
   </target>
</project>