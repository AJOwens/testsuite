<?xml version="1.0" encoding="UTF-8"?>
<project name="indexsee_htmlhelp" default="indexsee2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="indexsee2htmlhelp" if="run.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/IndexSee/index-see_testdata1.dita"/>
      <property name="output.dir" location="${dita.dir}/testresult/indexsee/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/indexsee/htmlhelp"/>
    </ant>
  </target>
</project>