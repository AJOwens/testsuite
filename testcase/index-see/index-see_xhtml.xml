<?xml version="1.0" encoding="UTF-8"?>
<project name="indexsee_xhtml" default="indexsee2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="indexsee2xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/IndexSee/index-see_testdata1.dita"/>
      <property name="output.dir" location="${dita.dir}/testresult/indexsee/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/indexsee/xhtml"/>
    </ant>
  </target>
</project>