<?xml version="1.0" encoding="UTF-8"?>
<project name="table-nocolnum_wordrtf" default="table-nocolnum2wordrtf" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="table-nocolnum2wordrtf">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/DITA2Wordrtf/CrossReference/simplemap.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/CrossReference/simplemap/Wordrtf"/>
      <property name="transtype" value="wordrtf"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/CrossReference/simplemap/Wordrtf"/>
    </ant>
  </target>
</project>