<?xml version="1.0" encoding="UTF-8" ?>
<project name="simpletable_wordrtf" default="simpletable2wordrtf" basedir=".">
    <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
    
    
    <target name="simpletable2wordrtf">
        <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <property name="args.input"
                value="${dita.dir}${file.separator}testdata${file.separator}DITA2Wordrtf${file.separator}Table${file.separator}simpletable.xml"/>
            <property name="output.dir"
                value="${dita.dir}${file.separator}testresult${file.separator}Table${file.separator}simpletable${file.separator}Wordrtf"/>
            <property name="transtype" value="wordrtf"/>
            <property name="dita.temp.dir" value="${dita.dir}${file.separator}temp${file.separator}Table${file.separator}simpletable${file.separator}Wordrtf"/>
            
        </ant>         
    </target>
</project>

