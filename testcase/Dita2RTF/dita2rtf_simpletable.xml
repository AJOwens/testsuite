<?xml version="1.0" encoding="UTF-8"?>
<project name="simpletable_wordrtf" default="simpletable2wordrtf" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="simpletable2wordrtf">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/DITA2Wordrtf/Table/simpletable.xml"/>
      <property name="output.dir" location="${dita.dir}/testresult/Table/simpletable/Wordrtf"/>
      <property name="transtype" value="wordrtf"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/Table/simpletable/Wordrtf"/>
    </ant>
  </target>
</project>