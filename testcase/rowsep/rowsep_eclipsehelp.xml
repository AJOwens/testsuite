<?xml version="1.0" encoding="UTF-8"?>
<project name="rowsep_eclipsehelp" default="rowsep2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="rowsep2eclipsehelp" if="run.eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/rowsep/rowsep.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/rowsep/eclipsehelp"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/rowsep/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
    </ant>
  </target>
</project>