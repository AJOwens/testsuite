<?xml version="1.0" encoding="UTF-8"?>
<project name="rowsep_eclipsehelp" default="rowsep2eclipsehelp" basedir=".">
   <dirname property="test.basedir" file="${ant.file.rowsep_eclipsehelp}"/>
   <property name="test.name" value="rowsep"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="rowsep2eclipsehelp" if="run.eclipsehelp" unless="skip.eclipsehelp">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${testdata.dir}/OldcasesTest/rowsep/rowsep.ditamap"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="eclipsehelp"/>
      </ant>
   </target>
</project>