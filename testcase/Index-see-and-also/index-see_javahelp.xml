<?xml version="1.0" encoding="utf-8"?><project name="indexsee_javahelp" default="indexsee2javahelp" basedir=".">
  
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  
  
  <target name="indexsee2javahelp">
    <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
      <property name="args.input" value="${dita.dir}${file.separator}testdata${file.separator}IndexSee${file.separator}index-see_testdata1.dita"/>

      <property name="output.dir" value="${dita.dir}${file.separator}testresult${file.separator}indexsee${file.separator}javahelp"/>
      <property name="transtype" value="javahelp"/>
      <property name="dita.temp.dir" value="${dita.dir}${file.separator}temp${file.separator}indexsee${file.separator}javahelp"/>
      <property name="clean.temp" value="no"/>
    </ant>
  </target>
</project>