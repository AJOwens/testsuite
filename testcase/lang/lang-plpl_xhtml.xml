<?xml version="1.0" encoding="UTF-8"?>
<project name="lang-plpl_xhtml" default="lang-plpl2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="lang-plpl2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/lang/lang-plpl.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/lang/plpl/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/lang/plpl/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.input.valfile"
                location="${testdata.dir}/OldcasesTest/lang/lang-ref.ditaval"/>
    </ant>
  </target>
</project>