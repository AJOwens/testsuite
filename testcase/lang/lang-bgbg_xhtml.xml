<?xml version="1.0" encoding="UTF-8"?>
<project name="lang-bgbg_xhtml" default="lang-bgbg2xhtml" basedir=".">
   <dirname property="test.basedir" file="${ant.file.lang-bgbg_xhtml}"/>
   <property name="test.name" value="lang"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="lang-bgbg2xhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${testdata.dir}/OldcasesTest/lang/lang-bgbg.dita"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="xhtml"/>
         <property name="dita.input.valfile"
                   location="${testdata.dir}/OldcasesTest/lang/lang-ref.ditaval"/>
      </ant>
   </target>
</project>