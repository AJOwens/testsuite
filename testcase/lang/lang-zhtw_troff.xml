<?xml version="1.0" encoding="UTF-8"?>
<project name="lang-zhtw_troff" default="lang-zhtw2troff" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="lang-zhtw2troff">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}OldcasesTest${file.separator}lang${file.separator}lang-zhtw.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}OldcasesTest${file.separator}lang${file.separator}zhtw${file.separator}troff"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}OldcasesTest${file.separator}lang${file.separator}zhtw${file.separator}troff"/>
      <property name="transtype" value="troff"/>
      <property name="dita.input.valfile"
                location="${testdata.dir}${file.separator}OldcasesTest${file.separator}lang${file.separator}lang-ref.ditaval"/>
    </ant>
  </target>
</project>