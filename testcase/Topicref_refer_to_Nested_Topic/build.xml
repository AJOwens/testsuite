<project name="Topicref_refer_to_Nested_Topic" default="all" basedir=".">
   <dirname property="test.basedir" file="${ant.file.Topicref_refer_to_Nested_Topic}"/>
   <property name="test.name" value="Topicref_refer_to_Nested_Topic"/>
   <dirname property="currentdir" file="${ant.file.Topicref_refer_to_Nested_Topic}"/>
   <target name="all">
      <echo message="${currentdir}" level="info"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="M_*.xml">
               <exclude name="M_Schema_Validation.xml"/>
            </fileset>
            <property name="test.name" value="Topicref_refer_to_Nested_Topic"/>
         </subant>
      </sequential>
   </target>
</project>