<?xml version="1.0" encoding="utf-8"?><project name="All_prohibited_xhtml" default="All_prohibitedxhtml" basedir=".">
    
    <!-- dita.dir should point to the toolkit's root directory -->
    <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
    <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
    
    <import file="${dita.dir}${file.separator}integrator.xml"/>
    
    <target name="All_prohibitedxhtml" depends="integrate">
        <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <!-- please refer to the toolkit's document for supported parameters, and 
           specify them base on your needs -->
            <property name="args.input" value="${testdata.dir}${file.separator}All_prohibited${file.separator}mp.ditamap"/>
            <property name="dita.temp.dir" value="${dita.dir}${file.separator}testresult${file.separator}12026${file.separator}All_prohibited${file.separator}xhtml${file.separator}temp"/>
            <property name="args.logdir" value="${dita.dir}${file.separator}testresult${file.separator}12026${file.separator}All_prohibited${file.separator}log"/>
            <property name="output.dir" value="${dita.dir}${file.separator}testresult${file.separator}12026${file.separator}All_prohibited${file.separator}xhtml"/>
            <property name="transtype" value="xhtml"/>
            <property name="clean.temp" value="no"/>
        </ant>
    </target>
</project>