<?xml version="1.0" encoding="UTF-8"?>
<project name="translation_eclipsehelp" default="translation2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="translation2eclipsehelp" if="run.eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/Translation/translation_testdata.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/translation_1/eclipsehelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/translation_1/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
    </ant>
  </target>
</project>