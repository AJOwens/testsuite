<?xml version="1.0" encoding="UTF-8" ?>

<project name="maptitle_javahelp" default="maptitle2javahelp" basedir=".">
  
  <property name="dita.dir" value="${basedir}${file.separator}.."/>
  
  <target name="maptitle2javahelp">
    <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
    <property name="args.input"
        value="${dita.dir}${file.separator}testdata${file.separator}Translation${file.separator}translation_testdata.ditamap"/>
      <property name="output.dir"
        value="${dita.dir}${file.separator}testresult${file.separator}translation_1${file.separator}javahelp"/>
      <property name="dita.temp.dir"
        value="${dita.dir}${file.separator}temp${file.separator}translation_1${file.separator}javahelp"/>
      <property name="transtype" value="javahelp"/>
    </ant>
  </target>
</project>


