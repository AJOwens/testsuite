<?xml version="1.0" encoding="UTF-8"?>
<project name="maptitle_javahelp" default="maptitle2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/.."/>
  <target name="maptitle2javahelp" if="run.javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/Translation/translation_testdata.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/translation_1/javahelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/translation_1/javahelp"/>
      <property name="transtype" value="javahelp"/>
    </ant>
  </target>
</project>