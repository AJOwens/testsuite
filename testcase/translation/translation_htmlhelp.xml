<?xml version="1.0" encoding="UTF-8"?>
<project name="maptitle_htmlhelp" default="maptitle2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="maptitle2htmlhelp" if="run.htmlhelp" unless="skip.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/Translation/translation_testdata.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/translation_1/htmlhelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/translation_1/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
    </ant>
  </target>
</project>