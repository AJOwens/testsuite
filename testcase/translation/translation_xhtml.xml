<?xml version="1.0" encoding="UTF-8"?>
<project name="translation_xhtml" default="translation2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="translation2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/Translation/translation_testdata.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/translation_1/xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/translation_1/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>