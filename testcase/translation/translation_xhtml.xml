<?xml version="1.0" encoding="UTF-8"?>
<project name="translation_xhtml" default="translation2xhtml" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <target name="translation2xhtml">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${dita.dir}${file.separator}testdata${file.separator}Translation${file.separator}translation_testdata.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}translation_1${file.separator}xhtml"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}translation_1${file.separator}xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>