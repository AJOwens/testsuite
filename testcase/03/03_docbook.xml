<?xml version="1.0" encoding="UTF-8"?>
<project name="03_docbook" default="032docbook" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="032docbook" if="run.docbook" unless="skip.docbook">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/03/03.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/03/docbook"/>
      <property name="output.dir" location="${dita.dir}/testresult/03/docbook"/>
      <property name="transtype" value="docbook"/>
    </ant>
  </target>
</project>