<?xml version="1.0" encoding="UTF-8"?>
<project name="03_troff" default="032troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="032troff" if="run.troff" unless="skip.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/03/03.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/03/troff"/>
      <property name="output.dir" location="${dita.dir}/testresult/03/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>