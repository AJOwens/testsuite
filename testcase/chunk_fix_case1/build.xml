<?xml version="1.0" encoding="UTF-8"?>
<project name="chunk_fix_case1" default="all">
  <dirname property="test.basedir" file="${ant.file.chunk_fix_case1}"/>
  <property name="test.name" value="${ant.project.name}"/>
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
  <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
  <target name="all" depends="chunk_fix_case1.preprocess"/>
  <target name="chunk_fix_case1.preprocess" if="run.preprocess" unless="skip.preprocess">
    <ant antfile="${dita.dir}/build.xml">
      <target name="build-init"/>
      <target name="preprocess"/>
      <property name="args.input" location="${test.basedir}/src/case1.ditamap"/>
      <property name="output.dir" location="${result.dir}/preprocess"/>
      <property name="dita.temp.dir" location="${temp.dir}/preprocess"/>
    </ant>
    <copy todir="${result.dir}/preprocess">
      <fileset dir="${temp.dir}/preprocess"/>
    </copy>
  </target>
</project>