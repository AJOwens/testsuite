<?xml version="1.0" encoding="UTF-8"?>
<project name="testprint_pdf" default="testprint2pdf" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="testprint2pdf">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/testprint/testprint.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/testprint/pdf"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/testprint/pdf"/>
      <property name="transtype" value="pdf"/>
    </ant>
  </target>
</project>