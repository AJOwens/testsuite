<?xml version="1.0" encoding="UTF-8"?>
<project name="indexbase_htmlhelp" default="indexbase2htmlhelp" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <target name="indexbase2htmlhelp">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${dita.dir}${file.separator}testdata${file.separator}IndexBase${file.separator}indexbase_testdata1.dita"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}indexbase_2${file.separator}htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}indexbase_2${file.separator}htmlhelp"/>
    </ant>
  </target>
</project>