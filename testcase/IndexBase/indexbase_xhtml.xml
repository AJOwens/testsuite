<?xml version="1.0" encoding="UTF-8" ?>

<project name="indexbase_xhtml" default="indexbase2xhtml" basedir=".">
  
<property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>

  <target name="indexbase2xhtml">
    <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
     <property name="args.input"
        value="${dita.dir}${file.separator}testdata${file.separator}IndexBase${file.separator}indexbase_testdata1.dita"/>

      <property name="output.dir"
        value="${dita.dir}${file.separator}testresult${file.separator}indexbase_4${file.separator}xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" value="${dita.dir}${file.separator}temp${file.separator}indexbase_4${file.separator}xhtml"/>
    
</ant>
  </target>
</project>

