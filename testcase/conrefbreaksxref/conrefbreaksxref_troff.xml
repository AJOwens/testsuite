<?xml version="1.0" encoding="UTF-8"?>
<project name="conrefbreaksxref_troff" default="conrefbreaksxref2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="conrefbreaksxref2troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/conrefbreaksxref/conrefbreaksxref.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/conrefbreaksxref/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/conrefbreaksxref/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>