<?xml version="1.0" encoding="UTF-8"?>
<project name="simplemap_troff" default="simplemap2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="simplemap2troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/simplemap/simplemap.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/simplemap/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/simplemap/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>