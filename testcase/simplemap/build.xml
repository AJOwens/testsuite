<project name="sample_all" default="all" basedir=".">
  <property name="currentdir" location="${basedir}"/>
  <target name="all">
    <echo message="${currentdir}"/>
    <sequential>
      <subant>
        <fileset dir="${currentdir}" includes="simplemap_*.xml"/>
      </subant>
    </sequential>
  </target>
</project>