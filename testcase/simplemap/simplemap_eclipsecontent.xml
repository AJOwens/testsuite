<?xml version="1.0" encoding="UTF-8"?>
<project name="simplemap_eclipsecontent" default="simplemap2eclipsecontent" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="simplemap2eclipsecontent">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/simplemap/simplemap.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/simplemap/eclipsecontent"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/simplemap/eclipsecontent"/>
      <property name="transtype" value="eclipsecontent"/>
    </ant>
  </target>
</project>