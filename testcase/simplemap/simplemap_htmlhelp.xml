<?xml version="1.0" encoding="UTF-8"?>
<project name="simplemap_htmlhelp" default="simplemap2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="simplemap2htmlhelp" if="run.htmlhelp" unless="skip.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/simplemap/simplemap.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/simplemap/htmlhelp"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/simplemap/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
    </ant>
  </target>
</project>