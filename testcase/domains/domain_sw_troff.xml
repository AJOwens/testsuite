<?xml version="1.0" encoding="UTF-8"?>
<project name="domain_sw_troff" default="domain_sw2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="domain_sw2troff" if="run.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/sw-d.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/domain_sw/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/domain_sw/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>