<project name="domains" default="all" basedir=".">
   <dirname property="currentdir" file="${ant.file.domains}"/>
   <target name="all">
      <echo message="${currentdir}"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="domain_*.xml"/>
         </subant>
      </sequential>
   </target>
</project>