<?xml version="1.0" encoding="UTF-8"?>
<project name="domain_hi_troff" default="domain_hi2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="domain_hi2troff" if="run.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/hi-d.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/domain_hi/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/domain_hi/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>