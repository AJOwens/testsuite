<?xml version="1.0" encoding="utf-8"?><project name="text_ph_element" default="text_ph_element" basedir=".">
    <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
    
    <target name="text_ph_element">
        
        <!--text_ph_element -->
        <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <property name="args.input" value="${dita.dir}${file.separator}testdata${file.separator}text_ph_element${file.separator}text_ph_element${file.separator}map.ditamap"/>
            <property name="output.dir" value="${dita.dir}${file.separator}testresult${file.separator}text_ph_element${file.separator}text_ph_element${file.separator}xhtml"/>
            <property name="transtype" value="xhtml"/>
            <property name="dita.temp.dir" value="${dita.dir}${file.separator}testresult${file.separator}text_ph_element${file.separator}text_ph_element${file.separator}xhtml${file.separator}temp"/>
            <property name="args.logdir" value="${dita.dir}${file.separator}testresult${file.separator}text_ph_element${file.separator}log"/>
            <property name="clean.temp" value="no"/>
        </ant> 
        
        <!--text_ph_reference -->
        <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <property name="args.input" value="${dita.dir}${file.separator}testdata${file.separator}text_ph_element${file.separator}text_ph_reference${file.separator}map.ditamap"/>
            <property name="output.dir" value="${dita.dir}${file.separator}testresult${file.separator}text_ph_element${file.separator}text_ph_reference${file.separator}xhtml"/>
            <property name="transtype" value="xhtml"/>
            <property name="dita.temp.dir" value="${dita.dir}${file.separator}testresult${file.separator}text_ph_element${file.separator}text_ph_reference${file.separator}xhtml${file.separator}temp"/>
            <property name="args.logdir" value="${dita.dir}${file.separator}testresult${file.separator}text_ph_element${file.separator}log"/>
            <property name="clean.temp" value="no"/>
        </ant> 
      
    </target>
</project>