<?xml version="1.0" encoding="utf-8"?><project name="keyref_pdf2" default="keyrefpdf2" basedir=".">
    
    <!-- dita.dir should point to the toolkit's root directory -->
    <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
    <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
    
    <import file="${dita.dir}${file.separator}integrator.xml"/>
    
    <target if="run.pdf" unless="skip.pdf" name="keyrefpdf2" depends="integrate">
        <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <!-- please refer to the toolkit's document for supported parameters, and 
           specify them base on your needs -->
            <property name="args.input" value="${testdata.dir}${file.separator}keyref${file.separator}map.ditamap"/>
            <property name="dita.temp.dir" value="${dita.dir}${file.separator}testresult${file.separator}12026${file.separator}keyref${file.separator}pdf2${file.separator}temp"/>
            <property name="args.logdir" value="${dita.dir}${file.separator}testresult${file.separator}12026${file.separator}keyref${file.separator}log"/>
            <property name="output.dir" value="${dita.dir}${file.separator}testresult${file.separator}12026${file.separator}keyref${file.separator}pdf2"/>
            <property name="transtype" value="pdf2"/>
            <property name="clean.temp" value="no"/>
        </ant>
    </target>
</project>