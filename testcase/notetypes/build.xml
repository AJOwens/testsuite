<?xml version="1.0" encoding="UTF-8"?>
<project name="notetypes" default="all">
   <dirname property="test.basedir" file="${ant.file.notetypes}"/>
   <property name="test.name" value="notetypes"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="all"
           depends="notetypes.xhtml, notetypes.eclipsehelp, notetypes.eclipsecontent, notetypes.javahelp, notetypes.htmlhelp, notetypes.pdf, notetypes.troff, notetypes.docbook, notetypes.wordrtf, notetypes.odt"/>
   <target name="notetypes.xhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="transtype" value="xhtml"/>
         <property name="args.input" location="${test.basedir}/src/notetypes.dita"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
      </ant>
   </target>
   <target name="notetypes.eclipsehelp" if="run.eclipsehelp" unless="skip.eclipsehelp">
