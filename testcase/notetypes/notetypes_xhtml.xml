<?xml version="1.0" encoding="UTF-8"?>
<project name="notetypes_xhtml" default="notetypes2xhtml" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="notetypes2xhtml">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}OldcasesTest${file.separator}notetypes.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}OldcasesTest${file.separator}notetypes${file.separator}xhtml"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}OldcasesTest${file.separator}notetypes${file.separator}xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>