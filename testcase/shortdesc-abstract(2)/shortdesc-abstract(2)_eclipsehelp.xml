<?xml version="1.0" encoding="UTF-8"?>
<project name="shortdesc/abstract(2)_eclipsehelp"
         default="shortdesc/abstract(2)2eclipsehelp"
         basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="shortdesc/abstract(2)2eclipsehelp" if="run.eclipsehelp" unless="skip.eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/ShortdescAbstract(2)/shortdesc-abstract(2)_testdata.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/shortdescabstract(2)_1/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/shortdescabstract(2)_1/eclipsehelp"/>
    </ant>
  </target>
</project>