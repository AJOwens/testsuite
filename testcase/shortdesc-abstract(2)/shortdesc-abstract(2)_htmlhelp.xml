<?xml version="1.0" encoding="UTF-8"?>
<project name="shortdesc/abstract(2)_htmlhelp" default="shortdesc/abstract(2)2htmlhelp"
         basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="shortdesc/abstract(2)2htmlhelp" if="run.htmlhelp" unless="skip.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/ShortdescAbstract(2)/shortdesc-abstract(2)_testdata.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/shortdescabstract(2)_2/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/shortdescabstract(2)_2/htmlhelp"/>
    </ant>
  </target>
</project>