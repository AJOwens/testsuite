<?xml version="1.0" encoding="UTF-8"?>
<project name="infotypes_eclipsehelp" default="infotypes2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="infotypes2eclipsehelp" if="run.eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/infotypes/infotypes.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/infotypes/eclipsehelp"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/infotypes/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
    </ant>
  </target>
</project>