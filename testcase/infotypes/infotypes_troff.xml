<?xml version="1.0" encoding="UTF-8"?>
<project name="infotypes_troff" default="infotypes2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="infotypes2troff" if="run.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/infotypes/infotypes.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/infotypes/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/infotypes/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>