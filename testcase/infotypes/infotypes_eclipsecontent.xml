<?xml version="1.0" encoding="UTF-8"?>
<project name="infotypes_eclipsecontent" default="infotypes2eclipsecontent" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="infotypes2eclipsecontent" if="run.eclipsecontent" unless="skip.eclipsecontent">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/infotypes/infotypes.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/infotypes/eclipsecontent"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/infotypes/eclipsecontent"/>
      <property name="transtype" value="eclipsecontent"/>
    </ant>
  </target>
</project>