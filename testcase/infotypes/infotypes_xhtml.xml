<?xml version="1.0" encoding="UTF-8"?>
<project name="infotypes_xhtml" default="infotypes2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="infotypes2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/infotypes/infotypes.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/infotypes/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/infotypes/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>