<?xml version="1.0" encoding="UTF-8"?>
<project name="infotypes_htmlhelp" default="infotypes2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="infotypes2htmlhelp" if="run.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/infotypes/infotypes.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/infotypes/htmlhelp"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/infotypes/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
    </ant>
  </target>
</project>