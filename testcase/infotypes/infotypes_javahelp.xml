<?xml version="1.0" encoding="UTF-8"?>
<project name="infotypes_javahelp" default="infotypes2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="infotypes2javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/infotypes/infotypes.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/infotypes/javahelp"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/infotypes/javahelp"/>
      <property name="transtype" value="javahelp"/>
    </ant>
  </target>
</project>