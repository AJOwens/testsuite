<?xml version="1.0" encoding="UTF-8"?>
<project name="22_TC4_xhtml" default="22_TC42xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="22_TC42xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/22/TC4.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/22/TC4/xhtml"/>
      <property name="output.dir" location="${dita.dir}/testresult/22/TC4/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.extname" value=".dita"/>
    </ant>
  </target>
</project>