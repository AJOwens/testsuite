<?xml version="1.0" encoding="UTF-8"?>
<project name="22_TC2_xhtml" default="22_TC22xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="22_TC22xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/22/TC2.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/22/TC2/xhtml"/>
      <property name="output.dir" location="${dita.dir}/testresult/22/TC2/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.extname" value=".dita"/>
    </ant>
  </target>
</project>