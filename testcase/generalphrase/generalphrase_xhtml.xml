<?xml version="1.0" encoding="UTF-8"?>
<project name="generalphrase_xhtml" default="generalphrase2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="generalphrase2xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/generalphrase.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/generalphrase/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/generalphrase/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>