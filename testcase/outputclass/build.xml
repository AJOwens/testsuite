<?xml version="1.0" encoding="UTF-8"?>
<project name="outputclass" default="outputclass2pdf" basedir=".">
   <property name="dita.dir" location="${basedir}/../.."/>
   <property name="testdata.dir" location="${dita.dir}/testdata"/>
   <target name="outputclass2pdf">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input"
                   location="${testdata.dir}/OldcasesTest/outputclass/outputclassspecific.ditamap"/>
         <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/outputclass/pdf"/>
         <property name="output.dir"
                   location="${dita.dir}/testresult/OldcasesTest/outputclass/pdf"/>
         <property name="transtype" value="pdf"/>
      </ant>
   </target>
</project>