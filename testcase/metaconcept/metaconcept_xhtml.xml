<?xml version="1.0" encoding="UTF-8"?>
<project name="metaconcept_xhtml" default="metaconcept2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="metaconcept2xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/metaconcept.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/metaconcept/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/metaconcept/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>