<?xml version="1.0" encoding="UTF-8"?>
<project name="metaconcept_xhtml" default="metaconcept2xhtml" basedir=".">
   <dirname property="test.basedir" file="${ant.file.metaconcept_xhtml}"/>
   <property name="test.name" value="metaconcept"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="metaconcept2xhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${testdata.dir}/OldcasesTest/metaconcept.dita"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="xhtml"/>
      </ant>
   </target>
</project>