<?xml version="1.0" encoding="UTF-8"?>
<project name="metaconcept_xhtml" default="metaconcept2xhtml" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="metaconcept2xhtml">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}OldcasesTest${file.separator}metaconcept.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}OldcasesTest${file.separator}metaconcept${file.separator}xhtml"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}OldcasesTest${file.separator}metaconcept${file.separator}xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>