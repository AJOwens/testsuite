<?xml version="1.0" encoding="UTF-8"?>
<project name="metaconcept_troff" default="metaconcept2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="metaconcept2troff" if="run.troff" unless="skip.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/metaconcept.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/metaconcept/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/metaconcept/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>