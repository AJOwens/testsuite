<?xml version="1.0" encoding="UTF-8"?>
<project name="indexsee_xhtml" default="indexsee2xhtml" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <target name="indexsee2xhtml">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${dita.dir}${file.separator}testdata${file.separator}IndexSee${file.separator}index-see_testdata1.dita"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}indexsee${file.separator}xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}indexsee${file.separator}xhtml"/>
    </ant>
  </target>
</project>