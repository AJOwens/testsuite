<?xml version="1.0" encoding="UTF-8" ?>
<project name="indexsee_htmlhelp" default="indexsee2htmlhelp" basedir=".">

  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  

  <target name="indexsee2htmlhelp">
    <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <property name="args.input"
        value="${dita.dir}${file.separator}testdata${file.separator}IndexSee${file.separator}index-see_testdata1.dita"/>

      <property name="output.dir"
        value="${dita.dir}${file.separator}testresult${file.separator}indexsee${file.separator}htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="dita.temp.dir" value="${dita.dir}${file.separator}temp${file.separator}indexsee${file.separator}htmlhelp"/>
    </ant>
  </target>
</project>
