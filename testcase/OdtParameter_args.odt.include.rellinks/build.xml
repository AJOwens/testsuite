<project name="OdtParameter_args.odt.include.rellinks" default="all" basedir=".">
   <dirname property="test.basedir"
            file="${ant.file.OdtParameter_args.odt.include.rellinks}"/>
   <property name="test.name" value="OdtParameter_args.odt.include.rellinks"/>
   <property name="currentdir" location="${test.basedir}"/>
   <target name="all">
      <echo message="${currentdir}" level="info"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="Value_*.xml"/>
            <property name="test.name" value="OdtParameter_args.odt.include.rellinks"/>
         </subant>
      </sequential>
   </target>
</project>