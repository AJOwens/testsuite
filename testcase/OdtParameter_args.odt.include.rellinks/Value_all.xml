<?xml version="1.0" encoding="UTF-8"?>
<project name="all" default="all" basedir=".">
   <dirname property="test.basedir" file="${ant.file.all}"/>
   <property name="test.name" value="OdtParameter_args.odt.include.rellinks"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="all">
      <ant antfile="${dita.dir}/build.xml" target="init"><!-- please refer to the toolkit's document for supported parameters, and specify them base on your needs --><property name="args.input" location="${test.basedir}/src/test.ditamap"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="clean.temp" value="yes"/>
         <property name="transtype" value="odt"/>
         <property name="args.odt.include.rellinks" value="all"/>
      </ant>
   </target>
</project>