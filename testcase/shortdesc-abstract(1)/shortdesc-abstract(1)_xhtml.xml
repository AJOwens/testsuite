<?xml version="1.0" encoding="UTF-8"?>
<project name="shortdesc/abstract(1)_xhtml" default="shortdesc/abstract(1)2xhtml"
         basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="shortdesc/abstract(1)2xhtml" if="run.xhtml" unless="skip.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/ShortdescAbstract(1)/shortdesc-abstract(1)_testdata.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/shortdescabstract(1)_4/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/shortdescabstract(1)_4/xhtml"/>
    </ant>
  </target>
</project>