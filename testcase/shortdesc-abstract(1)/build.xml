<?xml version="1.0" encoding="UTF-8"?>
<project name="shortdesc-abstract(1)" default="all" basedir=".">
   <dirname property="test.basedir" file="${ant.file.shortdesc-abstract(1)}"/>
   <property name="test.name" value="shortdesc-abstract(1)"/>
   <property environment="env"/>
   <property name="args.logdir" value="log"/>
   <property name="dita.extname" value=".xml"/>
   <import file="shortdesc-abstract(1)_eclipsehelp.xml"/>
   <import file="shortdesc-abstract(1)_htmlhelp.xml"/>
   <import file="shortdesc-abstract(1)_javahelp.xml"/>
   <import file="shortdesc-abstract(1)_xhtml.xml"/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="all"
           depends="shortdesc/abstract(1)2eclipsehelp, shortdesc/abstract(1)2htmlhelp, shortdesc/abstract(1)2javahelp, shortdesc/abstract(1)2xhtml"/>
</project>