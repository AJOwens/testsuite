<?xml version="1.0" encoding="utf-8"?><project name="body_section_div" default="body_section_div" basedir=".">
    <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
    
    <target name="body_section_div">
        
        <!--body_section_div-->
        <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <property name="args.input" value="${dita.dir}${file.separator}testdata${file.separator}body_section_div${file.separator}map.ditamap"/>
            <property name="output.dir" value="${dita.dir}${file.separator}testresult${file.separator}body_section_div${file.separator}xhtml"/>
            <property name="transtype" value="xhtml"/>
            <property name="dita.temp.dir" value="${dita.dir}${file.separator}testresult${file.separator}body_section_div${file.separator}xhtml${file.separator}temp"/>
            <property name="args.logdir" value="${dita.dir}${file.separator}testresult${file.separator}body_section_div${file.separator}log"/>
            <property name="clean.temp" value="no"/>
        </ant> 
    </target>
</project>