<project name="readme" default="all" basedir=".">
   <dirname property="test.basedir" file="${ant.file.readme}"/>
   <property name="test.name" value="readme"/>
   <dirname property="currentdir" file="${ant.file.readme}"/>
   <target name="all">
      <echo message="${currentdir}" level="info"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="${ant.project.name}_*.xml"/>
            <property name="test.name" value="readme"/>
         </subant>
      </sequential>
   </target>
</project>