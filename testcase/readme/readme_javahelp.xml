<?xml version="1.0" encoding="UTF-8"?>
<project name="readme_javahelp" default="readme2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="readme2javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/readme/DITA-readme.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/readme/javahelp"/>
      <property name="transtype" value="javahelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/readme/javahelp"/>
    </ant>
  </target>
</project>