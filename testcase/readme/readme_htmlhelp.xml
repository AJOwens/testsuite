<?xml version="1.0" encoding="UTF-8"?>
<project name="readme_htmlhelp" default="readme2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="readme2htmlhelp" if="run.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/readme/DITA-readme.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/readme/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/readme/htmlhelp"/>
    </ant>
  </target>
</project>