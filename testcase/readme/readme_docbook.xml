<?xml version="1.0" encoding="UTF-8"?>
<project name="readme_docbook" default="readme2docbook" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="readme2docbook">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/readme/DITA-readme.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/readme/docbook"/>
      <property name="transtype" value="docbook"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/readme/docbook"/>
    </ant>
  </target>
</project>