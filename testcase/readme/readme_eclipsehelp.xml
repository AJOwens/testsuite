<?xml version="1.0" encoding="UTF-8"?>
<project name="readme_eclipsehelp" default="readme2eclipsehelp" basedir=".">
   <dirname property="test.basedir" file="${ant.file.readme_eclipsehelp}"/>
   <property name="test.name" value="readme"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="readme2eclipsehelp" if="run.eclipsehelp" unless="skip.eclipsehelp">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${test.basedir}/src/DITA-readme.ditamap"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="eclipsehelp"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
      </ant>
   </target>
</project>