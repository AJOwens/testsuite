<?xml version="1.0" encoding="UTF-8"?>
<project name="readme_eclipsehelp" default="readme2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="readme2eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/readme/DITA-readme.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/readme/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/readme/eclipsehelp"/>
    </ant>
  </target>
</project>