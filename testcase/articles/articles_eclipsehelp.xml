<?xml version="1.0" encoding="UTF-8"?>
<project name="articles_eclipsehelp" default="articles2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="articles2eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/articles/DITA-articles.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/articles/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/articles/eclipsehelp"/>
    </ant>
  </target>
</project>