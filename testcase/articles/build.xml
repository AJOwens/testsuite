<project name="articles" default="all" basedir=".">
   <property name="currentdir" location="${basedir}"/>
   <target name="all">
      <echo message="${currentdir}"/>
      <sequential>
         <subant>
            <fileset dir="${currentdir}" includes="${ant.project.name}_*.xml"/>
         </subant>
      </sequential>
   </target>
</project>