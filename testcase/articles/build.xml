<project name="articles" default="all" basedir=".">
   <dirname property="test.basedir" file="${ant.file.articles}"/>
   <property name="test.name" value="articles"/>
   <dirname property="currentdir" file="${ant.file.articles}"/>
   <target name="all">
      <echo message="${currentdir}" level="info"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="${ant.project.name}_*.xml"/>
            <property name="test.name" value="articles"/>
         </subant>
      </sequential>
   </target>
</project>