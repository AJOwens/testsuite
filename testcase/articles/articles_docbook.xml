<?xml version="1.0" encoding="UTF-8"?>
<project name="articles_docbook" default="articles2docbook" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="articles2docbook">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/articles/DITA-articles.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/articles/docbook"/>
      <property name="transtype" value="docbook"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/articles/docbook"/>
    </ant>
  </target>
</project>