<?xml version="1.0" encoding="UTF-8"?>
<project name="articles_eclipsecontent" default="articles2eclipsecontent" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="articles2eclipsecontent">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/articles/DITA-articles.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/articles/eclipsecontent"/>
      <property name="transtype" value="eclipsecontent"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/articles/eclipsecontent"/>
    </ant>
  </target>
</project>