<?xml version="1.0" encoding="UTF-8"?>
<project name="articles_xhtml" default="articles2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="articles2xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/articles/DITA-articles.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/articles/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/articles/xhtml"/>
    </ant>
  </target>
</project>