<?xml version="1.0" encoding="UTF-8"?>
<project name="articles_javahelp" default="articles2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="articles2javahelp" if="run.javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/articles/DITA-articles.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/articles/javahelp"/>
      <property name="transtype" value="javahelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/articles/javahelp"/>
    </ant>
  </target>
</project>