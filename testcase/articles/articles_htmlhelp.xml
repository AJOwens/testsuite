<?xml version="1.0" encoding="UTF-8"?>
<project name="articles_htmlhelp" default="articles2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="articles2htmlhelp" if="run.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/articles/DITA-articles.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/articles/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/articles/htmlhelp"/>
    </ant>
  </target>
</project>