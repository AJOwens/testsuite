<?xml version="1.0" encoding="UTF-8"?>
<project name="image_xhtml" default="image2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="image2xhtml" if="run.xhtml" unless="skip.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/image.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/image/xhtml"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/image/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>