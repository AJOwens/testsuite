<?xml version="1.0" encoding="UTF-8"?>
<project name="image_troff" default="image2troff" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="image2troff">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}OldcasesTest${file.separator}image.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}OldcasesTest${file.separator}image${file.separator}troff"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}OldcasesTest${file.separator}image${file.separator}troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>