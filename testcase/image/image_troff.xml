<?xml version="1.0" encoding="UTF-8"?>
<project name="image_troff" default="image2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="image2troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/image.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/image/troff"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/image/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>