<project name="image" default="all" basedir=".">
   <dirname property="currentdir" file="${ant.file.image}"/>
   <target name="all">
      <echo message="${currentdir}"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="${ant.project.name}_*.xml"/>
         </subant>
      </sequential>
   </target>
</project>