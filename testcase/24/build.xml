<?xml version="1.0" encoding="UTF-8"?>
<project name="24" default="242xhtml" basedir=".">
   <property name="dita.dir" location="${basedir}/../.."/>
   <property name="testdata.dir" location="${dita.dir}/testdata"/>
   <target name="242xhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${testdata.dir}/24/24.xml"/>
         <property name="dita.temp.dir" location="${dita.dir}/temp/24/xhtml"/>
         <property name="output.dir" location="${dita.dir}/testresult/24/xhtml"/>
         <property name="transtype" value="xhtml"/>
         <property name="args.xsl" location="${testdata.dir}/24/24.xsl"/>
      </ant>
   </target>
</project>