<?xml version="1.0" encoding="UTF-8"?>
<project name="proptable_xhtml" default="proptable2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="proptable2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/proptable.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/proptable/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/proptable/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>