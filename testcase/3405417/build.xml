<?xml version="1.0" encoding="UTF-8"?>
<project name="3405417" default="all">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="all" depends="3405417.pdf"/>
  <target name="3405417.pdf" if="run.pdf" unless="skip.pdf">
