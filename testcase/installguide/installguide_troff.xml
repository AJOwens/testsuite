<?xml version="1.0" encoding="UTF-8"?>
<project name="installguide_troff" default="installguide2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="installguide2troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/installguide/installing-ditaot.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/installguide/troff"/>
      <property name="transtype" value="troff"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/installguide/troff"/>
    </ant>
  </target>
</project>