<?xml version="1.0" encoding="UTF-8"?>
<project name="installguide_xhtml" default="installguide2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="installguide2xhtml" if="run.xhtml" unless="skip.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/installguide/installing-ditaot.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/installguide/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/installguide/xhtml"/>
    </ant>
  </target>
</project>