<?xml version="1.0" encoding="UTF-8"?>
<project name="installguide_javahelp" default="installguide2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="installguide2javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/installguide/installing-ditaot.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/installguide/javahelp"/>
      <property name="transtype" value="javahelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/installguide/javahelp"/>
    </ant>
  </target>
</project>