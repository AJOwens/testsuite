<?xml version="1.0" encoding="UTF-8"?>
<project name="installguide_eclipsecontent" default="installguide2eclipsecontent"
         basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="installguide2eclipsecontent">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/installguide/installing-ditaot.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/installguide/eclipsecontent"/>
      <property name="transtype" value="eclipsecontent"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/installguide/eclipsecontent"/>
    </ant>
  </target>
</project>