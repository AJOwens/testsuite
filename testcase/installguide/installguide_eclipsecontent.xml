<?xml version="1.0" encoding="UTF-8"?>
<project name="installguide_eclipsecontent" default="installguide2eclipsecontent"
         basedir=".">
   <dirname property="test.basedir" file="${ant.file.installguide_eclipsecontent}"/>
   <property name="test.name" value="installguide"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="installguide2eclipsecontent" if="run.eclipsecontent"
           unless="skip.eclipsecontent">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${test.basedir}/src/installing-ditaot.ditamap"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="eclipsecontent"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
      </ant>
   </target>
</project>