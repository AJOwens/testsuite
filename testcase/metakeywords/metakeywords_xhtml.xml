<?xml version="1.0" encoding="UTF-8"?>
<project name="metakeywords_xhtml" default="metakeywords2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="metakeywords2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/metakeywords.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/metakeywords/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/metakeywords/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>