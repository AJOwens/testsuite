<?xml version="1.0" encoding="UTF-8"?>
<project name="xtrfxtrc_xhtml" default="xtrfxtrc2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="xtrfxtrc2xhtml" if="run.xhtml" unless="skip.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${dita.dir}/testdata/xtrfxtrc/xtrfxtrc.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/xtrfxtrc/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/xtrfxtrc/xhtml"/>
    </ant>
  </target>
</project>