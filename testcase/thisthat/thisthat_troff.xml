<?xml version="1.0" encoding="UTF-8"?>
<project name="thisthat_troff" default="thisthat2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="thisthat2troff" if="run.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/thisthat/thisthat.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/thisthat/troff"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/thisthat/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>