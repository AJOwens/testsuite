<?xml version="1.0" encoding="UTF-8"?>
<project name="thisthat_javahelp" default="thisthat2javahelp" basedir=".">
   <dirname property="test.basedir" file="${ant.file.thisthat_javahelp}"/>
   <property name="test.name" value="thisthat"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="thisthat2javahelp" if="run.javahelp" unless="skip.javahelp">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${test.basedir}/src/thisthat.ditamap"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="javahelp"/>
      </ant>
   </target>
</project>