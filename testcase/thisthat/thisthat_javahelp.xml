<?xml version="1.0" encoding="UTF-8"?>
<project name="thisthat_javahelp" default="thisthat2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="thisthat2javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/thisthat/thisthat.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/thisthat/javahelp"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/thisthat/javahelp"/>
      <property name="transtype" value="javahelp"/>
    </ant>
  </target>
</project>