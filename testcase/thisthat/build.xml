<project name="thisthat" default="all" basedir=".">
   <dirname property="test.basedir" file="${ant.file.thisthat}"/>
   <property name="test.name" value="thisthat"/>
   <dirname property="currentdir" file="${ant.file.thisthat}"/>
   <target name="all" description="Test description">
      <echo message="${currentdir}" level="info"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="${ant.project.name}_*.xml"/>
            <property name="test.name" value="thisthat"/>
         </subant>
      </sequential>
   </target>
</project>