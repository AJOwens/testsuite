<?xml version="1.0" encoding="UTF-8"?>
<project name="thisthat_eclipsehelp" default="thisthat2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="thisthat2eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${testdata.dir}/OldcasesTest/thisthat/thisthat.ditamap"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/thisthat/eclipsehelp"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/thisthat/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
    </ant>
  </target>
</project>