<?xml version="1.0" encoding="UTF-8"?>
<project name="dldlentry_troff" default="dldlentry2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="dldlentry2troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/dldlentry.dita"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/dldlentry/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/dldlentry/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>