<?xml version="1.0" encoding="utf-8"?><project name="chunk_fix_xhtml" default="chunk_fixhtml" basedir=".">
    
    <!-- dita.dir should point to the toolkit's root directory -->
    <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
    <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
    
    <import file="${dita.dir}${file.separator}integrator.xml"/>
    
    <target name="chunk_fixhtml" depends="integrate">
        
       
        <!-- chunk map7 -->
        <ant antfile="${dita.dir}${file.separator}build.xml" target="init">
            <!-- please refer to the toolkit's document for supported parameters, and 
           specify them base on your needs -->
            <property name="args.input" value="${testdata.dir}${file.separator}chunk_fix${file.separator}chunk${file.separator}map7.ditamap"/>
            <property name="dita.temp.dir" value="${dita.dir}${file.separator}testresult${file.separator}chunk_fix${file.separator}chunk${file.separator}map7${file.separator}eclipsehelp${file.separator}temp"/>
            <property name="args.logdir" value="${dita.dir}${file.separator}testresult${file.separator}chunk_fix${file.separator}chunk${file.separator}map7${file.separator}eclipsehelp${file.separator}log"/>
            <property name="output.dir" value="${dita.dir}${file.separator}testresult${file.separator}chunk_fix${file.separator}chunk${file.separator}map7${file.separator}eclipsehelp"/>
            <property name="transtype" value="eclipsehelp"/>
            <property name="clean.temp" value="no"/>
        </ant>
        
    </target>
</project>