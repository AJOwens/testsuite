<?xml version="1.0" encoding="UTF-8"?>
<project name="relativecolumnwidth" default="relativecolumnwidth" basedir=".">
  <description>Relative column width test</description>
   <dirname property="test.basedir" file="${ant.file.relativecolumnwidth}"/>
   <property name="test.name" value="relativecolumnwidth"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="relativecolumnwidth" depends="relativecolumnwidth2xhtml"/>
   <target name="relativecolumnwidth2xhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${test.basedir}/src/relativecolumnwidth.dita"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="xhtml"/>
      </ant>
   </target>
</project>