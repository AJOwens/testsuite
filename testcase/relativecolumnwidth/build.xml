<?xml version="1.0" encoding="UTF-8"?>
<project name="relativecolumnwidth_xhtml" default="relativecolumnwidth2xhtml"
         basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="relativecolumnwidth2xhtml">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}OldcasesTest${file.separator}relativecolumnwidth.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}OldcasesTest${file.separator}relativecolumnwidth${file.separator}xhtml"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}OldcasesTest${file.separator}relativecolumnwidth${file.separator}xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>