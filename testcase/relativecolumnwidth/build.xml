<?xml version="1.0" encoding="UTF-8"?>
<project name="relativecolumnwidth" default="relativecolumnwidth2xhtml" basedir=".">
   <property name="dita.dir" location="${basedir}/../.."/>
   <property name="testdata.dir" location="${dita.dir}/testdata"/>
   <target name="relativecolumnwidth2xhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input"
                   location="${testdata.dir}/OldcasesTest/relativecolumnwidth.dita"/>
         <property name="dita.temp.dir"
                   location="${dita.dir}/temp/OldcasesTest/relativecolumnwidth/xhtml"/>
         <property name="output.dir"
                   location="${dita.dir}/testresult/OldcasesTest/relativecolumnwidth/xhtml"/>
         <property name="transtype" value="xhtml"/>
      </ant>
   </target>
</project>