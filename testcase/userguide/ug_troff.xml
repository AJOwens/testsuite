<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_troff" default="userguide2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="userguide2troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/userguide/DITA-userguide.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/userguide/troff"/>
      <property name="output.dir" location="${dita.dir}/temp/userguide/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>