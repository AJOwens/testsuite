<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_docbook" default="userguide2docbook" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="userguide2docbook">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}userguide${file.separator}DITA-userguide.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}userguide${file.separator}docbook"/>
      <property name="transtype" value="docbook"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}userguide${file.separator}docbook"/>
    </ant>
  </target>
</project>