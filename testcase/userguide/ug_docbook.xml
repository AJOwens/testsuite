<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_docbook" default="userguide2docbook" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="userguide2docbook" if="run.docbook">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/userguide/DITA-userguide.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/userguide/docbook"/>
      <property name="transtype" value="docbook"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/userguide/docbook"/>
    </ant>
  </target>
</project>