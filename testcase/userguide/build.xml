<project name="userguide" default="all" basedir=".">
   <dirname property="test.basedir" file="${ant.file.userguide}"/>
   <property name="test.name" value="userguide"/>
   <dirname property="currentdir" file="${ant.file.userguide}"/>
   <target name="all">
      <echo message="${currentdir}" level="info"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="ug_*.xml"/>
            <property name="test.name" value="userguide"/>
         </subant>
      </sequential>
   </target>
</project>