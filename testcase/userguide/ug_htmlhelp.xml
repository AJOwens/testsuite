<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_htmlhelp" default="userguide2htmlhelp" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="userguide2htmlhelp">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}userguide${file.separator}DITA-userguide.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}userguide${file.separator}htmlhelp"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}temp${file.separator}userguide${file.separator}htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
    </ant>
  </target>
</project>