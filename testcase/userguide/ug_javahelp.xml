<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_javahelp" default="userguide2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="userguide2javahelp" if="run.javahelp" unless="skip.javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/userguide/DITA-userguide.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/userguide/javahelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/userguide/javahelp"/>
      <property name="transtype" value="javahelp"/>
    </ant>
  </target>
</project>