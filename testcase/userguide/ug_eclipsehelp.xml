<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_eclipsehelp" default="userguide2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="userguide2eclipsehelp" if="run.eclipsehelp" unless="skip.eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/userguide/DITA-userguide.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/userguide/eclipsehelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/userguide/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
    </ant>
  </target>
</project>