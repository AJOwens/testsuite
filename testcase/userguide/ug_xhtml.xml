<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_xhtml" default="userguide2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="userguide2xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/userguide/DITA-userguide.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/userguide/xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/userguide/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>