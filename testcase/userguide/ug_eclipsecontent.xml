<?xml version="1.0" encoding="UTF-8"?>
<project name="userguide_eclipsecontent" default="userguide2eclipsecontent" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="userguide2eclipsecontent">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}userguide${file.separator}DITA-userguide.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}userguide${file.separator}eclipsecontent"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}temp${file.separator}userguide${file.separator}eclipsecontent"/>
      <property name="transtype" value="eclipsecontent"/>
    </ant>
  </target>
</project>