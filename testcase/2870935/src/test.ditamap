<?xml version="1.0"?>
<!DOCTYPE map PUBLIC "-//OASIS//DTD DITA Map//EN" "map.dtd">
<map title="Keydef/Keyref Bug Sample">
    <topicref href="A.dita" />
    <topicref href="B.dita" keys="topicB" copy-to="topicB.dita"/>
    <reltable>
        <relrow>
            <relcell>
                <topicgroup collection-type="family">
                    <topicref href="A.dita" />
                    <topicref keyref="topicB" />
                </topicgroup>
            </relcell>
        </relrow>
    </reltable> 
    
</map>

