<?xml version="1.0" encoding="UTF-8"?>
<project name="DITAVAL_xhtml" default="DITAVAL2xhtml_6" basedir=".">
   <dirname property="test.basedir" file="${ant.file.DITAVAL_xhtml}"/>
   <property name="test.name" value="DITAVAL"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="DITAVAL2xhtml_6" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${test.basedir}/src/DITAVAL_testdata1.ditamap"/>
         <property name="transtype" value="xhtml"/>
         <property name="dita.input.valfile" location="${test.basedir}/src/DITAVAL_6.ditaval"/>
         <property name="output.dir" location="${result.dir}/xhtml/6"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml/6"/>
      </ant>
   </target>
</project>