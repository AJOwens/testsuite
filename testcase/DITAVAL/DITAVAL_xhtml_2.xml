<?xml version="1.0" encoding="UTF-8"?>
<project name="DITAVAL_xhtml" default="DITAVAL2xhtml_2" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="DITAVAL2xhtml_2" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/DITAVAL/DITAVAL_testdata1.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/DITAVAL/xhtml_2"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/DITAVAL/xhtml_2"/>
      <property name="dita.input.valfile"
                location="${dita.dir}/testdata/DITAVAL/DITAVAL_2.ditaval"/>
    </ant>
  </target>
</project>