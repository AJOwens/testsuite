<?xml version="1.0" encoding="UTF-8"?>
<project name="16_troff" default="162troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="162troff" if="run.troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/16/16.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/16/troff"/>
      <property name="output.dir" location="${dita.dir}/testresult/16/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>