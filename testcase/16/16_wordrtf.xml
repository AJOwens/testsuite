<?xml version="1.0" encoding="UTF-8"?>
<project name="16_wordrtf" default="162wordrtf" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="162wordrtf" if="run.wordrtf" unless="skip.wordrtf">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/16/16.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/16/wordrtf"/>
      <property name="output.dir" location="${dita.dir}/testresult/16/wordrtf"/>
      <property name="transtype" value="wordrtf"/>
    </ant>
  </target>
</project>