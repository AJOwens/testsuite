<?xml version="1.0" encoding="UTF-8"?>
<project name="table-nocolnum_xhtml" default="table-nocolnum2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="table-nocolnum2xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/table-nocolnum.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/table-nocolnum/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/table-nocolnum/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>