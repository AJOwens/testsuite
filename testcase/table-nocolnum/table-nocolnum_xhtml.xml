<?xml version="1.0" encoding="UTF-8"?>
<project name="table-nocolnum_xhtml" default="table-nocolnum2xhtml" basedir=".">
   <dirname property="test.basedir" file="${ant.file.table-nocolnum_xhtml}"/>
   <property name="test.name" value="table-nocolnum"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="table-nocolnum2xhtml" if="run.xhtml" unless="skip.xhtml">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${testdata.dir}/OldcasesTest/table-nocolnum.dita"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="xhtml"/>
      </ant>
   </target>
</project>