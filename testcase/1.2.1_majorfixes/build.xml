<project name="1.2.1_majorfixes" default="all" basedir=".">
   <property name="currentdir" location="${basedir}"/>
   <target name="all">
      <echo message="${currentdir}"/>
      <sequential>
         <subant>
            <fileset dir="${currentdir}" includes="C*.xml"/>
         </subant>
      </sequential>
      <sequential>
         <subant>
            <fileset dir="${currentdir}" includes="SF*.xml"/>
         </subant>
      </sequential>
   </target>
</project>