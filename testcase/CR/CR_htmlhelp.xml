<?xml version="1.0" encoding="UTF-8"?>
<project name="CR_htmlhelp" default="CR2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="CR2htmlhelp" if="run.htmlhelp" unless="skip.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/CR.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/CR/htmlhelp"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/CR/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
    </ant>
  </target>
</project>