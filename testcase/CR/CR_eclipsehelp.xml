<?xml version="1.0" encoding="UTF-8"?>
<project name="CR_eclipsehelp" default="CR2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="CR2eclipsehelp" if="run.eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/CR.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/CR/eclipsehelp"/>
      <property name="output.dir" location="${dita.dir}/testresult/OldcasesTest/CR/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
    </ant>
  </target>
</project>