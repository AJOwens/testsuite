<?xml version="1.0" encoding="UTF-8"?>
<project name="CR_eclipsecontent" default="CR2eclipsecontent" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="CR2eclipsecontent">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/CR.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/OldcasesTest/CR/eclipsecontent"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/CR/eclipsecontent"/>
      <property name="transtype" value="eclipsecontent"/>
    </ant>
  </target>
</project>