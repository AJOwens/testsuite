<?xml version="1.0" encoding="UTF-8"?>
<project name="indexseealso_xhtml" default="indexseealso2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="indexseealso2xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/IndexSeeAlso/index-see-also_testdata1.dita"/>
      <property name="output.dir" location="${dita.dir}/testresult/indexseealso/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/indexseealso/xhtml"/>
    </ant>
  </target>
</project>