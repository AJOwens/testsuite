<?xml version="1.0" encoding="UTF-8"?>
<project name="indexseealso_pdf" default="indexseealso2pdf" basedir=".">
   <dirname property="test.basedir" file="${ant.file.indexseealso_pdf}"/>
   <property name="test.name" value="index-see-also"/>
   <property name="dita.dir" location="${test.basedir}/../.."/>
   <property name="temp.dir" location="${dita.dir}/temp/${test.name}"/>
   <property name="result.dir" location="${dita.dir}/testresult/${test.name}"/>
   <target name="indexseealso2pdf" if="run.pdf" unless="skip.pdf">
      <ant antfile="${dita.dir}/build.xml">
         <property name="args.input" location="${test.basedir}/src/index-see-also_testdata1.dita"/>
         <property name="output.dir" location="${result.dir}/xhtml"/>
         <property name="transtype" value="pdf"/>
         <property name="dita.temp.dir" location="${temp.dir}/xhtml"/>
      </ant>
   </target>
</project>