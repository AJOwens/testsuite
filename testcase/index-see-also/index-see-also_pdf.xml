<?xml version="1.0" encoding="UTF-8"?>
<project name="indexseealso_pdf" default="indexseealso2pdf" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="indexseealso2pdf" if="run.pdf" unless="skip.pdf">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${dita.dir}/testdata/IndexSeeAlso/index-see-also_testdata1.dita"/>
      <property name="output.dir" location="${dita.dir}/testresult/indexseealso/pdf"/>
      <property name="transtype" value="pdf"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/indexseealso/pdf"/>
    </ant>
  </target>
</project>