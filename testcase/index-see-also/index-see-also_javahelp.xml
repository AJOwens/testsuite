<?xml version="1.0" encoding="UTF-8"?>
<project name="indexseealso_javahelp" default="indexseealso2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="indexseealso2javahelp" if="run.javahelp" unless="skip.javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/IndexSeeAlso/index-see-also_testdata1.dita"/>
      <property name="output.dir" location="${dita.dir}/testresult/indexseealso/javahelp"/>
      <property name="transtype" value="javahelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/indexseealso/javahelp"/>
    </ant>
  </target>
</project>