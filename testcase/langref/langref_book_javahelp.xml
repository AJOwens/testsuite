<?xml version="1.0" encoding="UTF-8"?>
<project name="langref_javahelp" default="langref2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="langref2javahelp" if="run.javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/langref/ditaref-book.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/langref/book/javahelp"/>
      <property name="transtype" value="javahelp"/>
      <property name="args.ftr" value="${testdata.dir}/langref/langref-footer.xml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/langref/book/javahelp"/>
    </ant>
  </target>
</project>