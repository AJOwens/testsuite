<?xml version="1.0" encoding="UTF-8"?>
<project name="langref_htmlhelp" default="langref2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="langref2htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/langref/ditaref-book.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/langref/book/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="args.ftr" value="${testdata.dir}/langref/langref-footer.xml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/langref/book/htmlhelp"/>
    </ant>
  </target>
</project>