<?xml version="1.0" encoding="UTF-8"?>
<project name="unordered-steps_xhtml" default="unordered-steps2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="unordered-steps2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/unordered-steps.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/unordered-steps/xhtml"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/unordered-steps/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>