<?xml version="1.0" encoding="UTF-8"?>
<project name="unordered-steps_troff" default="unordered-steps2troff" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="unordered-steps2troff">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/OldcasesTest/unordered-steps.dita"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/OldcasesTest/unordered-steps/troff"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/OldcasesTest/unordered-steps/troff"/>
      <property name="transtype" value="troff"/>
    </ant>
  </target>
</project>