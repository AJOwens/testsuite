<?xml version="1.0" encoding="UTF-8"?>
<project name="sample_xhtml" default="sample2xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="sample2xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/samples/hierarchy.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/samples/hierarchy/xhtml"/>
      <property name="transtype" value="xhtml"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/samples/hierarchy/xhtml"/>
    </ant>
  </target>
</project>