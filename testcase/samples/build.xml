<project name="samples" default="all" basedir=".">
   <dirname property="currentdir" file="${ant.file.samples}"/>
   <target name="all">
      <echo message="${currentdir}" level="info"/>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="sample_*.xml"/>
         </subant>
      </sequential>
      <sequential>
         <subant inheritall="true">
            <fileset dir="${currentdir}" includes="hierarchy_*.xml"/>
         </subant>
      </sequential>
   </target>
</project>