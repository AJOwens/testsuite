<?xml version="1.0" encoding="UTF-8"?>
<project name="sample_eclipse" default="sample2eclipse" basedir=".">
  <property name="dita.dir" value="${basedir}${file.separator}..${file.separator}.."/>
  <property name="testdata.dir" value="${dita.dir}${file.separator}testdata"/>
  <target name="sample2eclipse">
    <ant antfile="${dita.dir}${file.separator}build.xml">
      <property name="args.input"
                location="${testdata.dir}${file.separator}samples${file.separator}hierarchy.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}${file.separator}testresult${file.separator}samples${file.separator}hierarchy${file.separator}eclipse"/>
      <property name="transtype" value="eclipsehelp"/>
      <property name="dita.temp.dir"
                location="${dita.dir}${file.separator}temp${file.separator}samples${file.separator}hierarchy${file.separator}eclipse"/>
    </ant>
  </target>
</project>