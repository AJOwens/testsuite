<?xml version="1.0" encoding="UTF-8"?>
<project name="sample_docbook" default="sample2docbook" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="sample2docbook" if="run.docbook">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/samples/hierarchy.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/samples/hierarchy/docbook"/>
      <property name="transtype" value="docbook"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/samples/hierarchy/docbook"/>
    </ant>
  </target>
</project>