<?xml version="1.0" encoding="UTF-8"?>
<project name="sample_eclipsecontent" default="sample2eclipsecontent" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="sample2eclipsecontent" if="run.eclipsecontent" unless="skip.eclipsecontent">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/samples/sequence.ditamap"/>
      <property name="output.dir"
                location="${dita.dir}/testresult/samples/sequence/eclipsecontent"/>
      <property name="transtype" value="eclipsecontent"/>
      <property name="dita.temp.dir"
                location="${dita.dir}/temp/samples/sequence/eclipsecontent"/>
    </ant>
  </target>
</project>