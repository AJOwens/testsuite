<?xml version="1.0" encoding="UTF-8"?>
<project name="sample_javahelp" default="sample2javahelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="sample2javahelp" if="run.javahelp" unless="skip.javahelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/samples/hierarchy.ditamap"/>
      <property name="output.dir" location="${dita.dir}/testresult/samples/hierarchy/javahelp"/>
      <property name="transtype" value="javahelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/samples/hierarchy/javahelp"/>
    </ant>
  </target>
</project>