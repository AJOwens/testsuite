<?xml version="1.0" encoding="UTF-8"?>
<project name="indexbase_htmlhelp" default="indexbase2htmlhelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="indexbase2htmlhelp" if="run.htmlhelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/IndexBase/indexbase_testdata1.dita"/>
      <property name="output.dir" location="${dita.dir}/testresult/indexbase_2/htmlhelp"/>
      <property name="transtype" value="htmlhelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/indexbase_2/htmlhelp"/>
    </ant>
  </target>
</project>