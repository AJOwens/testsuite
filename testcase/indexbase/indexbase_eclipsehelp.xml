<?xml version="1.0" encoding="UTF-8"?>
<project name="indexbase_eclipsehelp" default="indexbase2eclipsehelp" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <target name="indexbase2eclipsehelp" if="run.eclipsehelp">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input"
                location="${dita.dir}/testdata/IndexBase/indexbase_testdata1.dita"/>
      <property name="output.dir" location="${dita.dir}/testresult/indexbase_1/eclipsehelp"/>
      <property name="transtype" value="eclipsehelp"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/indexbase_1/eclipsehelp"/>
    </ant>
  </target>
</project>