<?xml version="1.0" encoding="UTF-8"?>
<project name="04_TC2_xhtml" default="04_TC22xhtml" basedir=".">
  <property name="dita.dir" location="${basedir}/../.."/>
  <property name="testdata.dir" location="${dita.dir}/testdata"/>
  <target name="04_TC22xhtml" if="run.xhtml">
    <ant antfile="${dita.dir}/build.xml">
      <property name="args.input" location="${testdata.dir}/04/TC2/TC2.ditamap"/>
      <property name="dita.temp.dir" location="${dita.dir}/temp/04/TC2/xhtml"/>
      <property name="output.dir" location="${dita.dir}/testresult/04/TC2/xhtml"/>
      <property name="transtype" value="xhtml"/>
    </ant>
  </target>
</project>